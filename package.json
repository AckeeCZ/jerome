        "eslint-import-resolver-webpack": "^0.10.1",
        "eslint-loader": "^1.9.0",
        "eslint-plugin-jest": "^21.4.2",
        "jest": "^22.0.5",
        "less": "^3.8.1",
        "less-loader": "^4.1.0",
        "node-sass": "^4.9.3",
        "prettier-config-ackee": "0.0.5",
        "prop-types": "^15.6.0",
        "react": "^16.0.0",
        "react-dom": "^16.6.3",
        "react-intl": "^2.4.0",
        "react-redux": "^5.0.6",
