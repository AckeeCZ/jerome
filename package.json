{
    "name": "@ackee/jerome",
    "version": "0.0.1",
    "description": "Equipment for localizations at React-Redux-ReduxSaga stack",
    "main": "lib/index.js",
    "module": "es/index.js",
    "scripts": {
        "build:ts": "babel src --out-dir lib",
        "build": "npm run clean & npm run build:ts && npm run copy-styles",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "cross-env BABEL_ENV=development build-storybook -o build",
        "lint": "eslint \"src/**/*.{ts,tsx}\"",
        "test": "BABEL_ENV=test jest",
        "copy-styles": "copyfiles --up 1 src/**/*.scss styles",
        "clean": "rm -r lib & rm -r styles & rm -r build",
        "type-check": "tsc --noEmit",
        "type-check:watch": "npm run type-check -- --watch"
    },
    "author": "Lukáš Horák <lukas.horak@ackee.cz>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://gitlab.ack.ee/Web/frontend-toolkit"
    },
    "dependencies": {
        "antd": "^3.9.2",
        "react-display-name": "^0.2.4",
        "utility-types": "^2.1.0"
    },
    "peerDependencies": {
        "prop-types": "^15.6.0",
        "react": "^16.0.0",
        "react-intl": "^2.4.0",
        "react-redux": "^5.0.6",
        "redux-saga": "^0.16.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.1.2",
        "@babel/core": "^7.1.2",
        "@babel/plugin-proposal-class-properties": "^7.1.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
        "@babel/preset-env": "^7.1.0",
        "@babel/preset-react": "^7.0.0",
        "@babel/preset-typescript": "^7.1.0",
        "@storybook/addon-actions": "^3.4.11",
        "@storybook/addon-info": "^3.4.11",
        "@storybook/addon-links": "^3.4.11",
        "@storybook/addon-options": "^3.4.11",
        "@storybook/addon-storysource": "^3.4.11",
        "@storybook/react": "^3.4.11",
        "@types/react": "^16.7.13",
        "@types/react-dom": "^16.0.11",
        "@types/react-intl": "^2.3.14",
        "@types/react-redux": "^6.0.10",
        "@types/redux": "^3.6.0",
        "@types/storybook__react": "^3.0.9",
        "babel-jest": "^22.0.4",
        "babel-loader": "^8.0.4",
